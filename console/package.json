        "qunit": "^2.22.0",
        "qunit-dom": "^2.0.0",
        "recast": "^0.23.9",
        "stylelint": "^15.11.0",
        "stylelint-config-standard": "^34.0.0",
        "stylelint-prettier": "^4.1.0",
        "tailwindcss": "^3.4.10",
        "tracked-built-ins": "^3.3.0",
        "webpack": "^5.94.0"
    },
    "engines": {
        "node": ">= 18"
    },
    "ember": {
        "edition": "octane"
    },
    "pnpm": {
        "overrides": {
            "@fleetbase/ember-core": "latest",
            "@fleetbase/ember-ui": "latest",
            "@fleetbase/fleetops-data": "latest"
        }
    },
    "prettier": {
        "trailingComma": "es5",
        "tabWidth": 4,
        "semi": true,
        "singleQuote": true,
        "printWidth": 190,
        "overrides": [
            {
                "files": "*.hbs",
                "options": {
                    "singleQuote": false
                }
            }
        ]
    }
}
